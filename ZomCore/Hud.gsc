Welcome_User() {
  wait 5.0;
  self IPrintLn("^2[ System ]: ^7Welcome to our server!"); wait 5.0;
  self IPrintLn("^2[ System ]: ^7Use the bank via .b | .w | .d"); wait 2.0;
  self IPrintLn("^2[ System ]: ^7The bank stores money between rounds"); wait 2.0;
  self IPrintLn("^2[ System ]: ^7Your perk limit has been removed!"); wait 2.0;
  self IPrintLn("^2[ System ]: ^7Keep it friendly!"); wait 2.0;
  self IPrintLn("^2[ System ]: ^7Enjoy your stay, " + self.name); wait 2.0;
  self IPrintLn("^2[ System ]: ^7Join our Discord via: https://Discord.io/DokusCore");
}
