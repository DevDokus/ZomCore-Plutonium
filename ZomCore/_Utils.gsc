init() {}
////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////
DvarIsInitialized(dvarName) {
	result = GetDvar(dvarName);
	return result != "";
}
////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////
SetDvarIfNotInitialized(dvarName, dvarValue){
	if (!DvarIsInitialized(dvarName)) {
    SetDvar(dvarName, dvarValue);
  }
}
////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////
IsOwner() {
  if (self.guid == 3926232) {
    return true;
  } else {
    return false;
  }
}
